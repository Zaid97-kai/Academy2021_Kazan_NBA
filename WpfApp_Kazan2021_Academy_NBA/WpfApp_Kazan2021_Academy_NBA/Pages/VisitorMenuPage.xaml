      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp_Kazan2021_Academy_NBA.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Visitor Menu">

    <UniformGrid VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button x:Name="BtnTeams" Click="BtnTeams_Click">Teams</Button>
        <Button x:Name="BtnPlayers" Click="BtnPlayers_Click">Players</Button>
        <Button x:Name="BtnMatchups" Click="BtnMatchups_Click">Matchups</Button>
        <Button x:Name="BtnPhotos" Click="BtnPhotos_Click">Photos</Button>
    </UniformGrid>
</Page>
